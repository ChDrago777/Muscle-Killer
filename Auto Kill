    Parent = Frame,
    Size = UDim2.new(1, 0, 0, 36),
    Text = "ð€ð®ð­ð¨ ã…¤ðŠð¢ð¥ð¥ ã…¤ðŒð®ð¬ðœð¥ðž",
    Font = Enum.Font.GothamBold,
    TextSize = 18,
    TextColor3 = COLORS.Text,
    BackgroundTransparency = 1,
